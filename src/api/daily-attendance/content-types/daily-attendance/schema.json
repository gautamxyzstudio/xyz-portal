{
  "kind": "collectionType",
  "collectionName": "daily_attendances",
  "info": {
    "singularName": "daily-attendance",
    "pluralName": "daily-attendances",
    "displayName": "dailyAttendance",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "in": {
      "type": "time"
    },
    "out": {
      "type": "time"
    },
    "Date": {
      "type": "date"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "present",
        "absent",
        "late",
        "half-day",
        "leave",
        "short-leave"
      ],
      "default": "absent"
    },
    "notes": {
      "type": "text"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "daily_attendances"
    },
    "last_checkout_reminder": {
      "type": "datetime"
    },
     "checkin_started_at": {
      "type": "datetime"
    },
    "is_checked_in": {
      "type": "boolean",
      "default": false
    },
    "last_paused_at": {
      "type": "datetime"
    },
    "attendance_seconds": {
  "type": "integer",
  "default": 0
}

  }
}
