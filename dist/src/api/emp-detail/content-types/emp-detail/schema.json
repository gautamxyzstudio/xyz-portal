{
    "kind": "collectionType",
    "collectionName": "emp_details",
    "info": {
        "singularName": "emp-detail",
        "pluralName": "emp-details",
        "displayName": "userDetials",
        "description": ""
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "name": {
            "type": "string"
        },
        "designation": {
            "type": "string"
        },
        "empCode": {
            "type": "string"
        },
        "phoneNumber": {
            "type": "string"
        },
        "joiningDate": {
            "type": "date"
        },
        "Photo": {
            "type": "media",
            "multiple": true,
            "required": false,
            "allowedTypes": [
                "images",
                "files",
                "videos",
                "audios"
            ]
        },
        "user_detail": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "plugin::users-permissions.user",
            "inversedBy": "user_detial"
        },
        "isActive": {
            "type": "boolean",
            "default": true
        }
    }
}
