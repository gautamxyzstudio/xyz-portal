{
  "kind": "collectionType",
  "collectionName": "daily_tasks",
  "info": {
    "singularName": "daily-task",
    "pluralName": "daily-tasks",
    "displayName": "dailyTask"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "daily_work_log": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::daily-work-log.daily-work-log",
      "inversedBy": "daily_tasks",
      "required": true
    },

    "task_name": {
      "type": "string",
      "required": true
    },

    "estimated_minutes": {
      "type": "integer",
      "default": 0
    },

    "actual_minutes": {
      "type": "integer",
      "default": 0
    },

    "task_status": {
      "type": "enumeration",
      "enum": ["pending", "in_progress", "completed", "blocked"],
      "default": "pending"
    }
  }
}