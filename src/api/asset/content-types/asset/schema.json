{
  "kind": "collectionType",
  "collectionName": "assets",
  "info": {
    "singularName": "asset",
    "pluralName": "assets",
    "displayName": "Assets"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "AssetName": {
      "type": "string"
    },
    "AssetId": {
      "type": "string"
    },
    "Asset-Category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::asset-category.asset-category"
    },
    "Model": {
      "type": "string"
    },
    "Brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::asset-brand.asset-brand"
    },
    "Condition": {
      "type": "enumeration",
      "enum": [
        "New",
        "Old"
      ]
    },
    "Available": {
      "type": "boolean",
      "default": true
    },
    "SerialNumber": {
      "type": "string"
    },
    "AssignedTo": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "assets"
    }
  }
}