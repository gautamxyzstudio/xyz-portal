{
  "kind": "collectionType",
  "collectionName": "work_logs",
  "info": {
    "singularName": "work-log",
    "pluralName": "work-logs",
    "displayName": "Work Log",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "tasks": {
      "type": "json"
    },
    "active_task_id": {
      "type": "integer"
    },
    "total_time_taken": {
      "type": "integer",
      "default": 0
    },
    "work_date": {
      "type": "date",
      "required": true
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "work_logs"
    },
    "daily_task": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::daily-task.daily-task",
      "inversedBy": "work_logs"
    }
  }
}
