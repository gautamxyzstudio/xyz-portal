{
    "kind": "collectionType",
    "collectionName": "emp_details",
    "info": {
        "singularName": "emp-detail",
        "pluralName": "emp-details",
        "displayName": "userDetials",
        "description": ""
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "name": {
            "type": "string"
        },
        "designation": {
            "type": "string"
        },
        "empCode": {
            "type": "string"
        },
        "phoneNumber": {
            "type": "string"
        },
        "joiningDate": {
            "type": "date"
        },
        "Photo": {
            "type": "media",
            "multiple": true,
            "required": false,
            "allowedTypes": [
                "images",
                "files",
                "videos",
                "audios"
            ]
        },
        "status": {
            "type": "enumeration",
            "enum": [
                "active",
                "deactive"
            ]
        },
        "leave_status": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "api::leave-status.leave-status",
            "mappedBy": "emp_detail"
        },
        "leave_balance": {
            "type": "integer",
            "required": true
        },
        "daily_attendances": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::daily-attendance.daily-attendance",
            "mappedBy": "emp_detail"
        },
        "user_detail": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "plugin::users-permissions.user",
            "inversedBy": "user_detial"
        }
    }
}
