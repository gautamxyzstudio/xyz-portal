{
  "kind": "collectionType",
  "collectionName": "emp_details",
  "info": {
    "singularName": "emp-detail",
    "pluralName": "emp-details",
    "displayName": "emp_details",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "designation": {
      "type": "string"
    },
    "empCode": {
      "type": "string"
    },
    "phoneNumber": {
      "type": "string"
    },
    "Photo": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "user_detail": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "user_detial"
    },
    "status": {
      "type": "boolean",
      "default": true
    },
    "coverImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "date_of_birth": {
      "type": "date"
    },
    "joining_announced": {
      "type": "boolean",
      "default": false
    },
    "active_blogs": {
      "type": "boolean",
      "default": false
    },
    "joinig_date": {
      "type": "date"
    }
  }
}
