{
    "kind": "collectionType",
    "collectionName": "emp_details",
    "info": {
        "singularName": "emp-detail",
        "pluralName": "emp-details",
        "displayName": "EmpDetail",
        "description": ""
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "name": {
            "type": "string"
        },
        "designation": {
            "type": "string"
        },
        "empCode": {
            "type": "string"
        },
        "phoneNumber": {
            "type": "string"
        },
        "email": {
            "type": "email"
        },
        "joiningDate": {
            "type": "date"
        },
        "Photo": {
            "type": "media",
            "multiple": true,
            "required": false,
            "allowedTypes": [
                "images",
                "files",
                "videos",
                "audios"
            ]
        },
        "status": {
            "type": "enumeration",
            "enum": [
                "active",
                "deactivate"
            ]
        },
        "leave_status": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "api::leave-status.leave-status",
            "mappedBy": "emp_detail"
        },
        "leave_balance": {
            "type": "integer",
            "required": true
        },
        "daily_attendances": {
            "type": "relation",
            "relation": "manyToMany",
            "target": "api::daily-attendance.daily-attendance",
            "inversedBy": "emp_details"
        }
    }
}
