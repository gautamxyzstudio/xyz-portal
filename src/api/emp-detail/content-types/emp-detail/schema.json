{
  "kind": "collectionType",
  "collectionName": "emp_details",
  "info": {
    "singularName": "emp-detail",
    "pluralName": "emp-details",
    "displayName": "EmpDetail",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "designation": {
      "type": "string"
    },
    "empCode": {
      "type": "string"
    },
    "phoneNumber": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "joiningDate": {
      "type": "date"
    },
    "Photo": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "daily_attendance": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::daily-attendance.daily-attendance",
      "mappedBy": "emp_detail"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "active",
        "deactivate"
      ]
    },
    "leave_status": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::leave-status.leave-status",
      "mappedBy": "emp_detail"
    },
    "leave_balance": {
      "type": "integer",
      "required": true
    }
  }
}
